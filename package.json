  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server",
    "prestart:docker": "npm install && npm run build",
    "start-docker": "cross-env NODE_ENV=development webpack-dev-server --history-api-fallback --host 0.0.0.0 --port 2992 --hot --inline",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack",
    "postbuild": "npm run sitemap",
    "clean": "rimraf dist",
    "sitemap": "node bin/build_sitemap.js",
    "lint": "eslint src --max-warnings 0",
    "pretest": "npm run lint",
    "preserve": "npm run build",
    "serve": "live-server --no-browser --entry-file=index.html --proxy=/api:$API_URL/api dist/",
    "test": "NODE_ENV=test mocha-webpack --webpack-config webpack.config-test.js \"src/**/*test.js\"",
    "test:watch": "NODE_ENV=test mocha-webpack --webpack-config webpack.config-test.js --watch \"src/**/*test.js\"",
    "fixup-lock": "sed -i '' -e 's/http:\\/\\//https:\\/\\//g' package-lock.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run fixup-lock"
    }
  },
  "lint-staged": {
    "package-lock.json": [
      "npm run fixup-lock",
      "git add"
    ]
  }
